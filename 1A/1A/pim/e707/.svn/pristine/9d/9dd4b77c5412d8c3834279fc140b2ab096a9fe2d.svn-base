with Arbre_Binaire;
procedure Test_Arbre_Binaire is
      Package Arbre_Binaire_Entier is
            new Arbre_Binaire (T_Cle => Integer);
    use Arbre_Binaire_Entier;

    -- Tester la proc√©dure Initialiser_Arbre
    procedure Test_Initialiser_Arbre(Arbre: in out T_Arbre_Binaire) is
    begin
        Initialiser_Arbre(Arbre);
        pragma Assert (Est_Vide_Arbre(Arbre));
    end Test_Initialiser_Arbre;

    procedure Test_Ajouter is
        Arbre1: T_Arbre_Binaire;
    begin
        Initialiser_Arbre(Arbre1);
        Ajouter(Arbre1,10,10, True);
        pragma Assert(Est_Present(Arbre1,10));
    end Test_Ajouter;

    procedure Test_Supprimer_Noeud is
        Arbre1: T_Arbre_Binaire;
    begin
        Initialiser_Arbre(Arbre1);
        Ajouter(Arbre1,10,10, True);
        Supprimer_Noeud(Arbre1,10);
        pragma Assert(not Est_Present(Arbre1,10));
    end Test_Supprimer_Noeud;

    Abr: T_Arbre_Binaire;
    begin

        Test_Initialiser_Arbre(Abr);
        Test_Ajouter;
        Test_Supprimer_Noeud;
    end Test_Arbre_Binaire;
