with arbre_binaire
generic
    Type T_Information is private;

Package Registre is

    Type T_Registre is limited private;

    procedure Initialiser(R: out T_Registre);

    procedure Ajouter(R: in out T_Registre, I: in T_Information);

    procedure Supprimer(R:in out T_Registre, Cle:in T_Cle);

    procedure Modifier(R:in out T_Registre, Cle:in T_Cle, I: in T_Information);

private

    Type T_Cellule_Registre;
    Type T_Registre is access T_Cellule_Registre;
    Type T_Cellule_Registre is
        record
            Cle: T_Cle;
            Information : T_Information;
            suivant: T_Registre;
        end record;

