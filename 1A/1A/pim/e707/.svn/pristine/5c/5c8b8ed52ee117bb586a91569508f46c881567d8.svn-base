generic
    Type T_Information is private;
    type T_Cle_r is private;

Package Registre is

    Type T_Registre is limited private;

    procedure Initialiser_R(R: out T_Registre);

    procedure Ajouter_R(R: in out T_Registre;Cle: in T_Cle; I: in T_Information);

    procedure Supprimer_R(R:in out T_Registre; Cle:in T_Cle);

    procedure Modifier_R(R:in out T_Registre; Cle:in T_Cle; I: in T_Information);

private

    Type T_Cellule_Registre;
    Type T_Registre is access T_Cellule_Registre;
    Type T_Cellule_Registre is
        record
            Cle: T_Cle_r;
            Information : T_Information;
            suivant: T_Registre;
        end record;
end Registre;


