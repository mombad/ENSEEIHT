project TP2 is

   for Main use ("arbre_binaire.ads", "registre.ads");

   package Builder is
      for Default_Switches ("ada") use ("-s");
   end Builder;

   package Compiler is
      for Default_Switches ("ada") use ("-gnata", "-g", "-gnatwa");
   end Compiler;

   package Binder is
      for Default_Switches ("ada") use ("-E");
   end Binder;

end TP2;

